{
	// Place your snippets for latex here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"Theorem": {
		"prefix": "\\thm",
		"body": ["\\begin{Theorem}{$1}{$2}",
				"$3",
				"\\end{Theorem}"],
		"description": "Theorem Box"
	},
	"Theorem-Chap": {
		"prefix": "\\thmc",
		"body": ["\\begin{theorem}{$1}{$2}",
				"$3",
				"\\end{theorem}"],
		"description": "Theorem Box With Chapter Number"
	},
	"Definition": {
		"prefix": "\\def",
		"body": ["\\begin{Definition}{$1}{$2}",
				"$3",
				"\\end{Definition}"],
		"description": "Definition Box"
	},
	"Definition-Chap": {
		"prefix": "\\defc",
		"body": ["\\begin{definition}{$1}{$2}",
				"$3",
				"\\end{definition}"],
		"description": "Definition Box With Chapter Number"
	},
	"Lemma": {
		"prefix": "\\lem",
		"body": ["\\begin{lemma}{$1}{$2}",
				"$3",
				"\\end{lemma}"],
		"description": "Lemma Box"
	},
	"Lemma-Chap": {
		"prefix": "\\lemc",
		"body": ["\\begin{Lemma}{$1}{$2}",
				"$3",
				"\\end{Lemma}"],
		"description": "Lemma Box With Chapter Number"
	},
	"Hyperref":{
		"prefix": "\\hpref",
		"body": "\\hyperref[$1]{$2}",
		"description": "Hyperref Reference"
	},
	"Proof":{
		"prefix": "\\pf",
		"body": [
			"\\begin{proof}",
			"$1",
			"\\end{proof}"
		],
		"description": "Proof Environment"
	}
}