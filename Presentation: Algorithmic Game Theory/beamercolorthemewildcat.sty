\definecolor{nudarkred}{RGB}{216, 088, 032}

% Enumerate item color 
\setlist[enumerate]{label={\ifnum\@itemdepth=1\relax
  \color{alerted text}\else
  \color{wcprimary}\fi\theenumi.}}

% Itemize item color (bullet)
\setlist[itemize]{label=\color{wcprimary}$\bullet$}
%\setlist[itemize]{label=\color{wcprimary}\textbullet}

% Change alerted text color to wcalerted (default is nudarkorange)
\setbeamercolor{alerted text}{fg=wcalerted}

% Turn off shading for block title when rounded=true (by default, rounded = false)
\makeatletter
\pgfdeclareverticalshading[lower.bg,upper.bg]{bmb@transition}{200cm}{%
  color(0pt)=(lower.bg); color(4pt)=(lower.bg); color(4pt)=(upper.bg)}

% Change block title background to wcprimary and body bg to wcprimary10
\setbeamercolor{block title}{fg=white,bg=wcprimary}
\setbeamercolor{block body}{parent=normal text,fg=black,bg=wcprimary10}

% Change the alertblock title background to alerted text color
\setbeamercolor{block title alerted}{fg=white,bg=wcalerted}
\setbeamercolor{block body alerted}{fg=black,bg=wcalerted!10}

% Change the exampleblock title background to greens
\setbeamercolor{block title example}{fg=white,bg=wcexample}
\setbeamercolor{block body example}{fg=black,bg=wcexample!10}

% Change cption text color to wcprimary
\setbeamercolor{caption name}{fg=wcprimary}

% Change color of toc text to wcprimary
\setbeamercolor{section in toc}{fg=wcprimary}

% Ensure continuation text is white (for allowframebreaks)
\setbeamercolor{frametitle continuation}{fg=wcprimary}

% Set framtitle color to wcprimary
\setbeamercolor{frametitle}{fg=wcprimary}

\mode
<all>